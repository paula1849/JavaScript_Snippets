//Random insult generator

var randomBodyParts = ['nose', 'hair', 'feet', 'bum'];
var randomAdjectives = ['stinky', 'bulging', 'pointy', 'moist'];
var randomWords = ['skunk', 'booger', 'fried', 'pickles'];

var randomBodyPart = randomBodyParts[Math.floor(Math.random() * 3)];
var randomAdjective = randomAdjectives[Math.floor(Math.random() * 3)];
var randomWord = randomWords[Math.floor(Math.random()) * 3];

console.log('Your ' + randomBodyPart + ' is like a ' + randomAdjective + ' ' + randomWord);
